vazio escreveoli ( texto r)
    escrever "oli",r
fim escreveola

vazio escreveola ( texto o)
    escreveoli(o)
    escrever "ola",o
fim escreveola

vazio escreveole(texto b)
    escreveola(b)
    escrever "ole",b
fim escreveole

programa
    texto xpto <- "teste"
    escreveole(xpto)
fim programa